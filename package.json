{
  "name": "photo-catalog-prettifier",
  "version": "1.0.0",
  "description": "",
  "bin": "build/cli/index.js",
  "type": "module",
  "scripts": {
    "build": "rm -rf build; tsc -p .",
    "watch": "tsc -p . --watch",
    "test": "jest src",
    "test:dev": "DEBUG={DEBUG:-'pcp:*'} jest src --watch",
    "validate": "tsc -p . --noEmit",
    "cli": "env $(cat .env | xargs) ts-node --esm src/cli/index.ts",
    "buildData": "npm run cli -- buildData",
    "server": "env $(cat .env | xargs) ts-node --esm src/server/index.ts",
    "server:dev": "nodemon --watch \"src/**\" --ext \"ts\" --ignore \"src/**/*.test.ts\" --exec \"npm run server\"",
    "clear": "rm -rf data/*"
  },
  "keywords": [],
  "author": "Joshuan <joshuan.chel@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.20.2",
    "cli-progress": "3.12.0",
    "debug": "4.3.4",
    "ejs": "3.1.8",
    "execa": "7.0.0",
    "express": "4.18.2",
    "imagemagick": "0.1.3",
    "lodash": "4.17.21",
    "luxon": "3.2.1",
    "md5-file": "5.0.0",
    "p-limit": "4.0.0",
    "phash-imagemagick": "1.0.1",
    "pino": "8.11.0",
    "pino-http": "8.3.3",
    "pino-pretty": "9.4.0",
    "uuid": "9.0.0",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@jest/globals": "29.4.3",
    "@types/cli-progress": "3.11.0",
    "@types/debug": "4.1.7",
    "@types/ejs": "3.1.2",
    "@types/express": "4.17.17",
    "@types/lodash": "4.14.191",
    "@types/luxon": "3.2.0",
    "@types/node": "18.13.0",
    "@types/uuid": "9.0.1",
    "@types/yargs": "17.0.22",
    "jest": "29.4.3",
    "nodemon": "2.0.21",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
